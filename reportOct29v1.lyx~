\begin_deeper
\begin_layout Standard
\begin_inset Formula 
\begin{align}
C_{N\times n} & =S_{N\times N}\Sigma_{N\times n}(V_{n\times n})^{T}\nonumber \\
\tilde{C} & =Z_{N\times p}\Sigma_{p\times p}(V_{p\times n})^{T}\nonumber \\
 & \approx C\nonumber \\
\tilde{C}^{T} & =(\Sigma_{p\times p}(V_{p\times n})^{T})^{T}S_{N\times p}^{T}\label{eq:3}
\end{align}

\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $N$
\end_inset

: sample size, 
\begin_inset Formula $p$
\end_inset

: reduced dimension.
\end_layout

\begin_layout Standard
Apply (
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
ref{eq:3})
\end_layout

\end_inset

 to 
\begin_inset Formula $C=Y_{n\times N}^{T}$
\end_inset

, denote 
\begin_inset Formula $Z_{p\times N}=S_{N\times p}^{T}$
\end_inset

, 
\begin_inset Formula $A=(\Sigma_{p\times p}(V_{p\times n})^{T})^{T}=V_{p\times n}\Sigma_{p\times p}^{T}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\tilde{Y}_{n\times N}=AZ_{p\times N}
\end{equation}

\end_inset


\begin_inset Note Note
status collapsed

\begin_layout Plain Layout
Find 
\begin_inset Formula $A$
\end_inset

:
\begin_inset Formula 
\begin{align*}
||\boldsymbol{y}-A\boldsymbol{z}|| & \le\varepsilon\\
\dim\boldsymbol{z} & \le5
\end{align*}

\end_inset


\end_layout

\end_inset


\begin_inset Formula 
\begin{align}
\boldsymbol{y}(t) & =A\boldsymbol{z}(t)\\
\boldsymbol{z}(t) & =A^{-1}\boldsymbol{y}(t)
\end{align}

\end_inset


\end_layout

