\end_layout

\end_inset


\end_layout

\begin_deeper
\begin_layout Plain Layout
\begin_inset Formula 
\begin{align*}
e(t) & =B\frac{J(\boldsymbol{f})}{J(\boldsymbol{y})}(\boldsymbol{y}(t),\boldsymbol{k})\boldsymbol{\varepsilon}(t)-B\dot{\boldsymbol{\varepsilon}}(t)
\end{align*}

\end_inset


\end_layout

\begin_layout Itemize
When 
\begin_inset Formula $\boldsymbol{f}$
\end_inset

 is linear or affine, 
\begin_inset Formula $\frac{J(\boldsymbol{f})}{J(\boldsymbol{y})}$
\end_inset

 does not depend on 
\begin_inset Formula $\boldsymbol{y}$
\end_inset


\end_layout

\end_deeper
\end_inset


\end_layout

