\begin_layout Subsection
Perturbation analysis (initial thoughts)
\end_layout

\begin_layout Frame
\begin_inset Argument 4
status open

\begin_layout Plain Layout
Dimension reduction using linear transformations: perturbation analysis
\end_layout

\end_inset


\end_layout

\begin_deeper
\begin_layout Standard
\begin_inset Formula $A,B$
\end_inset

 are linear transformations that we need to define
\begin_inset Formula 
\begin{align*}
\boldsymbol{y}(t) & =A\boldsymbol{z}(t)+\boldsymbol{\varepsilon}(t)\\
\boldsymbol{z}(t) & =B(\boldsymbol{y}(t)-\boldsymbol{\varepsilon}(t))\\
\dot{\boldsymbol{z}}(t) & =B(\dot{\boldsymbol{y}}(t)-\dot{\boldsymbol{\varepsilon}}(t))\\
 & =B(\boldsymbol{f}(\boldsymbol{y}(t),\boldsymbol{k})-\dot{\boldsymbol{\varepsilon}}(t))\\
 & =B(\boldsymbol{f}(A\boldsymbol{z}(t)+\boldsymbol{\varepsilon}(t),\boldsymbol{k})-\dot{\boldsymbol{\varepsilon}}(t))\\
\dot{\boldsymbol{z}}(t)-B\boldsymbol{f}(A\boldsymbol{z}(t),\boldsymbol{k}) & =B(\boldsymbol{f}(A\boldsymbol{z}(t)+\boldsymbol{\varepsilon}(t),\boldsymbol{k})-\dot{\boldsymbol{\varepsilon}}(t))-B\boldsymbol{f}(A\boldsymbol{z}(t),\boldsymbol{k})\\
 & =B\left[\boldsymbol{f}(A\boldsymbol{z}(t)+\boldsymbol{\varepsilon}(t),\boldsymbol{k})-\boldsymbol{f}(A\boldsymbol{z}(t),\boldsymbol{k})\right]-B\dot{\boldsymbol{\varepsilon}}(t)\\
 & \approx B\frac{J(\boldsymbol{f})}{J(\boldsymbol{y})}(A\boldsymbol{z}(t)+\boldsymbol{\varepsilon}(t),\boldsymbol{k})\boldsymbol{\varepsilon}(t)-B\dot{\boldsymbol{\varepsilon}}(t)+o(||\boldsymbol{\varepsilon}(t)||)\\
 & \text{ (linear approximation at }A\boldsymbol{z}(t)+\boldsymbol{\varepsilon}(t),\text{ not }A\boldsymbol{z}(t))
\end{align*}

\end_inset


\end_layout

\end_deeper
\begin_layout Separator

\end_layout

\begin_layout Frame
\begin_inset Argument 4
status open

\begin_layout Plain Layout
Dimension reduction using linear transformations: perturbation analysis
\end_layout

\end_inset


\end_layout

\begin_deeper
\begin_layout Standard
\begin_inset Formula 
\begin{align}
\dot{\boldsymbol{z}}(t)-B\boldsymbol{f}(A\boldsymbol{z}(t),\boldsymbol{k}) & \approx\boldsymbol{e}(t)\nonumber \\
\boldsymbol{e}(t) & =B\frac{J(\boldsymbol{f})}{J(\boldsymbol{y})}(\boldsymbol{y}(t),\boldsymbol{k})\boldsymbol{\varepsilon}(t)-B\dot{\boldsymbol{\varepsilon}}(t)\\
\dot{\boldsymbol{z}}(t)-B\boldsymbol{f}(A\boldsymbol{z}(t),\boldsymbol{k}) & =0\label{eq:1-1}\\
\dot{\boldsymbol{z}}(t)-B\boldsymbol{f}(A\boldsymbol{z}(t),\boldsymbol{k}) & =\boldsymbol{e}(t)\label{eq:2-1}
\end{align}

\end_inset


\begin_inset Note Note
status open

\begin_layout Plain Layout
we need to esimate the difference between the solution of 
\begin_inset ERT
status open

\begin_layout Plain Layout

(
\backslash
ref{eq:1-1},
\backslash
ref{eq:2-1})
\end_layout

\end_inset

 and how that difference will be accumulated in 
\begin_inset Formula $\boldsymbol{y}=A\boldsymbol{z}$
\end_inset

.
 
\end_layout

\end_inset


\end_layout

\end_deeper
